package Pattern;

import java.util.Scanner;

public class Pattern06 {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		int i = 1;
		while(i<=n) {
			int j = 1;
			int var = 1;
			while(j<=i) {
				j=i;
				System.out.print(var);
				j++;
				var++;
			}
			System.out.println();
			i++;
			
		}
				

	}

}
