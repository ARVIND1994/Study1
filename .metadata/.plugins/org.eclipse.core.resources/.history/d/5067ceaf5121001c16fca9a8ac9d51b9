package Pattern;

import java.util.Scanner;

public class Pattern11 {

	public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int n = s.nextInt();
        int i = n;
        while(i>=1) {
        	int j = i;
        	 
        	
        	char character = (char)('A' + j -1);
        	while(j<=n) {
        		System.out.print((char)('A' + j -1));
        		//character++;
        		j++;
        		
        	}
        	System.out.println();
        	i--;
        }
        
	}

}
