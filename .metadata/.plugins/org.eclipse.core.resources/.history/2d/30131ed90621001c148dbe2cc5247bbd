package Pattern;

import java.util.Scanner;

public class Pattern08 {

	public static void main(String[] args) {
         Scanner s = new Scanner(System.in);
         int n = s.nextInt();
         int i = 1;
         while(i<=n) {
        	 int j = 1;
        	 char character = (char)('A' + i -1);
        	 while(j<=n) {
        		 System.out.print(character);
        		 character = (char) (character+1);
        		 j++;
        		 
        	 }
        	 System.out.println();
        	 
        	 i++;
         }
        		 
	}

}
