package Pattern;

import java.util.Scanner;

public class Pattern05 {

	public static void main(String[] args) {
       Scanner s = new Scanner(System.in);
       int n = s.nextInt();
       int i = 0 ;
       while(i<=n) {
    	   int j = 1;
    	   while(j<=i) {
    		   System.out.print("*");
    	   }
    	   System.out.println();
    	   i++;
       }
	}

}
