package Pattern;

import java.util.Scanner;

public class Pattern13 {

	public static void main(String[] args) {

		
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		int i = 1;
		while(i<=n) {
			int j = 1;
			 int val = 1;
			while(j<= n-i) {
				System.out.print(" ");
				j++;
			}
				while(val<=n) {
					System.out.print(val);
					val++;
				}
			System.out.println();
			i++;
			
		}
	}

}
